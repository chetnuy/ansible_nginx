---
- hosts: all
  become: true
  
  roles:
    - geerlingguy.nginx
    
  vars:
    nginx_vhosts:
      - listen: 80
        server_name: "dtln.com"
        extra_parameters: |
          location / {
              proxy_pass http://127.0.0.1:8081;
              server_name_in_redirect on;
              proxy_set_header Host $host;
              proxy_set_header X-Real-IP $remote_addr;
              proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
